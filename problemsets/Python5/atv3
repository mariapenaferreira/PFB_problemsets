#operadores de conjuntos

a = {'abracadabra'}
print(a)
a1 = set('abracadabra')
print(a1)

# se, 
b = set('alacazam')

#Diferença
print(a1 - b) 

#União
print(a1 | b)

#Intersecção
print(a1 & b)

#Diferença simétrica
print(a1 ^ b)

dna = ""
dna_set = set(dna)
print(dna_set)

#criando um dicionário vazio para guardar as sequencias a seguir

nucleot = {}
for nuc in set(dna):
	nucleot[nuc] = dna.count(nuc)
print('contagem de nucleotídeos: ', nucleot)

#ou 

nucleo = {}
for nt in set(dna):
    if nt not in nucleo:
        nucleo[nt] = 1 
    else:
        nucleo[nt] += 1

print("Contagem de nucleotídeos:", nucleo)

#ou 

nucleoti = {}
for nt in set(dna):
	if nt in nucleoti:
		nucleoti[nt] += 1
	else:
		nucleoti[nt] = 1;
print("Contagem de nucleotídeos:", nucleoti)
